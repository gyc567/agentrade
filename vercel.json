{
  "version": 2,
  "framework": "vite",
  "buildCommand": "cd web && npm run build",
  "outputDirectory": "web/dist",
  "installCommand": "cd web && npm install",
  "devCommand": "cd web && npm run dev",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://nofx-gyc567.replit.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
